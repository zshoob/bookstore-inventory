{
  "manifest_version": 2,

  "name": "Listing Scraper",
  "description": "",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  }
}
